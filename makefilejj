all: main.o helpers.o parsing.o runs.o linked_list.o cJSON.o
	gcc -o main -I/usr/include main.o -lSDL2 helpers.o parsing.o runs.o shell.o linked_list.o cJSON.o

main.o: main.c includes.h cJSON.h
	gcc -c main.c

helpers.o: helpers.c includes.h
	gcc -c helpers.c

parsing.o: parsing.c parsing.h includes.h
	gcc -c parsing.c

runs.o: runs.c includes.h
	gcc -c runs.c

linked_list.o: linked_list.c linked_list.h includes.h cJSON.h
	gcc -c linked_list.c

cJSON.o: cJSON.c cJSON.h
	gcc -c cJSON.c
clean:
	rm -rf *.o
